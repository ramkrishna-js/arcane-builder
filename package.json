{
  "name": "arcane-builder",
  "version": "0.1.0",
  "description": "File-based Discord bot framework CLI",
  "main": "src/index.js",
  "bin": {
    "arcane": "bin/arcane.js"
  },
  "type": "commonjs",
  "scripts": {
